<?xml version="1.0" encoding="utf-8" ?>
<!--가독성을 위해 dataPacket을 제일 위부터 쓰기!!-->
<PDL>
	<!--DataPackets-->
	<dataPacket name ="VectorPacket">
		<float name="x"/>
		<float name="y"/>
		<float name="z"/>
	</dataPacket>
	
	<dataPacket name ="QuaternionPacket">
		<float name="x"/>
		<float name="y"/>
		<float name="z"/>
		<float name="w"/>
	</dataPacket>
	
	<dataPacket name="RoomInfoPacket">
		<int name ="roomId"/>
		<int name ="maxCount"/>
		<int name ="currentCount"/>
		<string name="roomName"/>
		<string name="hostName"/>
	</dataPacket>
	
	<dataPacket name="PlayerNamePacket">
		<string name ="nickName"/>
		<int name="index"/>
	</dataPacket>

	<dataPacket name="PlayerInitPacket">
		<int name="index"/>
		<int name="modelIndex"/>
		<string name="name"/>
		<VectorPacket name ="position"/>
		<QuaternionPacket name ="rotation"/>
	</dataPacket>
	
	<!--Packets-->
	<packet name="C_RoomEnter">
		<int name ="roomId"/>
	</packet>
	<packet name="C_GameStart">
	</packet>
	<packet name="C_SetName">
		<string name="name"/>
	</packet>
	
	<packet name="S_RoomEnter">
		<!--<PlayerInfoPacket name="newPlayer"/>-->
	</packet>
	<packet name="S_RoomEnterFirst">
		<!--나중에 추가-->
		<int name="myIndex"/>
		<list name="inits">
			<PlayerInitPacket name="att"/>
		</list>
	</packet>
	
	<packet name="C_RoomExit">
	</packet>
	
	<packet name="S_RoomExit">
		<int name="Index"/>
	</packet>
	
	<packet name="C_CreateRoom">
		<string name="roomName"/>
		<int name="maxCount"/>
	</packet>
	
	<packet name ="S_RoomList">
		<list name ="roomInfos">
			<roomInfoPacket name ="att"/>
		</list>
	</packet>
	
	<packet name="S_PacketResponse">
		<ushort name="responsePacket"/>
		<bool name="success"/>
	</packet>
	
	<packet name ="C_RoomList">
	</packet>
	
	<packet name="S_Chat">
		<string name="pName"/>
		<string name="text"/>
	</packet>
	
	<packet name="C_Chat">
		<string name="text"/>
	</packet>

	<packet name="C_Move">
		<VectorPacket name="position"/>
		<VectorPacket name="direction"/>
		<float name="speed"/>
	</packet>
	<packet name="C_Rotate">
		<QuaternionPacket name="rotation"/>
	</packet>
	<packet name="C_ChangeModel">
		<int name="modelIndex"/>
	</packet>
	<packet name="S_Rotate">
		<int name="index"/>
		<QuaternionPacket name="rotation"/>
	</packet>
	<packet name="S_Move">
		<int name="index"/>
		<float name="speed"/>
		<VectorPacket name="position"/>
		<VectorPacket name="direction"/>
	</packet>
	<packet namme="S_ModelChange">
		<int name ="index"/>
		<int name="modelIndex"/>
	</packet>
	<packet name="S_SyncTimer">
		<float name="time"/>
	</packet>
	<packet name="S_UpdateRoomState">
		<ushort name="state"/>
	</packet>
</PDL>